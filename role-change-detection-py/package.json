{
  "name": "role-change-detection-beta",
  "displayName": "Role Change Detection (beta)",
  "version": "0.2.0",
  "engines": {
    "node": ">=20"
  },
  "description": "Detecting role changes in smart contracts",
  "longDescription": "Bot that detects role changes in smart contracts",
  "repository": "https://github.com/forta-network/starter-kits/tree/main/role-change-detection-py",
  "licenseUrl": "https://github.com/forta-network/starter-kits/tree/main/role-change-detection-py/LICENSE.md",
  "promoUrl": "https://forta.org",
  "chainIds": [
    1, 8453
  ],
  "chainSettings": {
    "1": {
      "shards": 15,
      "target": 3
    },
    "8453": {
      "shards": 28,
      "target": 3
    },
    "default": {
      "shards": 7,
      "target": 3
    }
  },
  "scripts": {
    "postinstall": "python3 -m pip install -r requirements_dev.txt",
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e py --exec \"python3 ./src/agent.py\"",
    "start:prod": "node ./src/agent.js",
    "tx": "forta-bot run --tx",
    "block": "forta-bot run --block",
    "range": "forta-bot run --range",
    "alert": "forta-bot run --alert",
    "sequence": "forta-bot run --sequence",
    "file": "forta-bot run --file",
    "publish": "forta-bot publish",
    "info": "forta-bot info",
    "logs": "forta-bot logs",
    "push": "forta-bot push",
    "disable": "forta-bot disable",
    "enable": "forta-bot enable",
    "keyfile": "forta-bot keyfile",
    "stake": "forta-bot stake",
    "test": "python3 -m pytest"
  },
  "devDependencies": {
    "forta-bot-cli": "file:forta-bot-cli-0.2.0.tgz"
  }
}